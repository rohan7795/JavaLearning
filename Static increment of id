package demo;

public class Customer {

	static int id = 1001;
	int customerId;
	String name;
	Customer(){
		this.customerId = id;
		id++;
		System.out.println("id :" +id);
	}
	
	public static void main(String[] args) {
		
		Customer a1 = new Customer();
		Customer a2= new Customer();
		Customer a3 = new Customer();
		Customer a4 = new Customer();
		Customer a5 = new Customer();
		Customer a6 = new Customer();
		Customer a7 = new Customer();
		

		System.out.println(a1.customerId);
		System.out.println(a2.customerId);
		System.out.println(a3.customerId);
		System.out.println(a4.customerId);
		System.out.println(a5.customerId);
		System.out.println(a6.customerId);
		System.out.println(a7.customerId);
	
		
		
	}
}
